
[mysqladmin]
user=root

[mysqld]
basedir=/Applications/BitNami/mysql
datadir=/Applications/BitNami/mysql/data
port=3306
socket=/Applications/BitNami/mysql/tmp/mysql.sock
tmpdir=/Applications/BitNami/mysql/tmp
max_allowed_packet=16M
bind-address=127.0.0.1

character-set-server=UTF8
collation-server=utf8_general_ci
[mysqld_safe]
mysqld=mysqld.bin

[client]
port=3306
socket=/Applications/BitNami/mysql/tmp/mysql.sock

default-character-set=UTF8
[manager]
port=3306
socket=/Applications/BitNami/mysql/tmp/mysql.sock
pid-file=/Applications/BitNami/mysql/tmp/manager.pid
default-mysqld-path=/Applications/BitNami/mysql/bin/mysqld.bin

